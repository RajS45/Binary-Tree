-->> Print path from root to leaf node.
class Solution {
    public List<List<Integer>> allRootToLeaf(TreeNode root) {
        List<List<Integer>> allPaths = new ArrayList<>();
        List<Integer> path = new ArrayList<>();
        func(root , path , allPaths);
        return allPaths;
    }
    public void func(TreeNode root , List<Integer>path , List<List<Integer>> allPaths){
        if(root == null){
            return ;
        }
        path.add(root.data);
        if(root.left == null && root.right == null){
            allPaths.add(new ArrayList<>(path));
        }
        else{
            func(root.left , path,allPaths);
            func(root.right , path , allPaths);
        }
        path.remove(path.size() -1) ;
    }
}
